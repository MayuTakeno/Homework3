<h1>Users</h1>
<table>
  <thead class="user-thead">
    <tr>
      <th class="user-table-image">image</th>
      <th class="user-table-name">name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody class="user-tbody">
    <tr>
      <% @users.each do |user| %>
        <td class="user-prof">
          <%= user.name %>
        </td>
        <td class="link-to-show">
          <%= link_to "Show", user_path(user.id) %>
        </td>
        <% end %>
    </tr>
  </tbody>
</table>

<%= render '/books/list', book: @book, user: @user %>

